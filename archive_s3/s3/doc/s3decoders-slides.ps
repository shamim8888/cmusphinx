20 dict begin

/inch { 72 mul } def

/cmulogosquare		% stack: ---
{
    % do a square at 0 0
    %
    % do a border
    0.75 setlinewidth
    0 0 moveto
    0 1 inch lineto
    1 inch 1 inch lineto
    1 inch 0 lineto
    closepath
    stroke

% the gray inside the square
     /fstr 256 string def
     0 1 255 { fstr exch dup put } for
     1 inch 1 inch scale
     256 1 inch 8 [256 0 0 -72 0 72] {fstr} image
} bind def

/cmulogo	% stack x y ---
{
    % do a 1 inch cmu logo at x y
    gsave
	% move the coordinate system to x y
	translate
	% First the tilted square
	gsave
	    14 rotate
	    cmulogosquare
	grestore

	% now the Carnegie-Mellon
	/Times-Bold findfont 21 scalefont setfont
	0.22 inch 0.73 inch moveto
	(Carnegie) show
	0.22 inch 0.45 inch moveto
	(Mellon) show
    grestore
} bind def

% Main procedure

0.875 0.875 scale

0.2 inch 0 cmulogo
end
